{
  "name": "mr-incredible-uncanny-faces",
  "displayName": "Incredibly In Your Face",
  "description": "In Your Face, shows you Incredible 'Uncanny Faces' that correlate to the number of errors in your code!",
  "version": "2.0",
  "publisher": "Antonin Meunier",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization"
  ],
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "keywords": [
    "uncanny",
    "incredible",
    "faces",
    "canny"
  ],
  "__metadata": {
    "publisherDisplayName": "VirejDasani"
  },
  "activationEvents": [
    "onCommand:mr-incredible-uncanny.openview.focus",
    "onView:mr-incredible-uncanny.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "Incredibly In Your Face",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "mr-incredible-uncanny.openview",
          "name": "In Your Face",
          "contextualTitle": "In Your Face"
        }
      ]
    },
    "commands": [
      {
        "command": "mr-incredible-uncanny.openview.focus",
        "category": "In Your Face",
        "title": "Open Sidebar",
        "icon": "$(clear-all)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.21",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "cjs": "^0.0.11"
  }
}
